{
  "name": "@crowdhouse/coreui",
  "version": "27.10.1",
  "description": "Core UI elements",
  "author": "Crowdhouse FrontEnd Team",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/crowdhouse/crowdhouse-ui"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/crowdhouse/crowdhouse-ui"
  },
  "homepage": "https://github.com/crowdhouse/crowdhouse-ui",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/*"
  ],
  "scripts": {
    "start": "react-scripts start",
    "build": "dotenv -- bash -c 'npm run build:library && npm run build:storybook'",
    "build:library": "rm -rf dist && npx babel --extensions '.ts,.tsx' --ignore '**/*.test.tsx' src/lib --out-dir dist --copy-files --no-copy-ignored && find dist -name '__snapshots__' | xargs rm -r && npm run types",
    "build:storybook": "npx build-storybook -o dist-storybook --loglevel error --quiet --disable-telemetry",
    "test": "react-scripts test --watch --colors --maxWorkers=75%",
    "test:ci": "react-scripts test ---colors --watchAll=false --maxWorkers=75% --coverage=true --ci --bail --silent",
    "test:staged": "npm test -- --ci --watchAll=false --bail --findRelatedTests --coverage=false",
    "types": "tsc -p tsconfig.types.json && tsc -p tsconfig.types-libs.json",
    "lint": "npx concurrently --kill-others-on-fail --names \"tsc,lint-js,lint-scss,prettier\" -c \"bgBlue.bold,bgGreen.bold,bgYellow.bold,bgWhite.bold\" \"npx tsc\" \"npm run lint:js\" \"npm run lint:scss\" \"npm run prettier:check\"",
    "lint:js": "eslint --quiet ./src --ext js,ts,jsx,tsx,json",
    "lint:scss": "stylelint src/**/*.module.scss",
    "develop": "rm -rf dist && npx babel --extensions '.ts,.tsx' --watch --ignore '**/*.test.tsx' src/lib --out-dir dist --copy-files --no-copy-ignored && find dist -name '__snapshots__' | xargs rm -r && npm run types",
    "validate": "npx concurrently --kill-others-on-fail --names \"tsc,lint-js,lint-scss,prettier,test\" -c \"bgBlue.bold,bgGreen.bold,bgYellow.bold,bgWhite.bold,bgMagenta.bold\" \"npx tsc\" \"npm run lint:js\" \"npm run lint:scss\" \"npm run prettier:check\" \"npm run test:ci\" ",
    "prettier": "prettier '{src,tests}/**/*.{js,jsx,ts,tsx,json,css,scss}'",
    "prettier:write": "npm run prettier -- --write",
    "prettier:check": "npm run prettier -- --check",
    "storybook": "start-storybook -p 6006",
    "deploy": "aws s3 cp dist-storybook $S3_STORYBOOK_BUCKET/ --recursive --quiet",
    "setup": "npm ci",
    "publish:package": "npx semantic-release",
    "chromatic": "npx chromatic --project-token=8298131fbf0f --exit-once-uploaded"
  },
  "eslintConfig": {
    "extends": [
      "@crowdhouse/eslint-config-react"
    ],
    "rules": {
      "testing-library/prefer-screen-queries": 0,
      "testing-library/no-container": 0,
      "graphql/no-deprecated-fields": 0,
      "react/require-default-props": 0
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": ">=14.6.0"
  },
