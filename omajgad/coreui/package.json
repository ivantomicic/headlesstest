  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json}": [
      "eslint --fix",
      "npm run prettier:write"
    ],
    "src/**/*.{js,jsx,ts,tsx}": [
      "npm run test:staged"
    ]
  },
  "storybook-deployer": {
    "gitUsername": "ciCrowdhouse",
    "gitEmail": "ciCrowdhouse@crowdhouse.ch",
    "commitMessage": "Deploy Storybook"
  },
  "jest": {
    "clearMocks": true,
    "resetMocks": false,
    "coverageReporters": [
      "lcov"
    ],
    "collectCoverageFrom": [
      "**/*.js",
      "**/*.jsx",
      "**/*.ts",
      "**/*.tsx",
      "!**/*.d.ts",
      "!**/*.spec.js*",
      "!**/*.spec.ts*",
      "!**/*.test.ts*",
      "!**/*.test.js*",
      "!**/__mocks__/**",
      "!src/apollo/generated.ts",
      "!src/apollo/helpers.ts",
      "!**/*.graphql.d.ts"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
